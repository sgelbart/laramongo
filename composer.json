{
	"require": {
		"laravel/framework": "dev-master#fc9760ad63e2421e484410394fd97e393fb21962",
		"way/generators": "dev-master",
		"navruzm/lmongo": "dev-master#d673d7762028ab53a54f227caa8882830e80c427",
		"keboola/csv": "1.0.9",
		"zizaco/confide": "dev-master",
		"zizaco/lessy": "dev-master",
		"jasonlewis/basset": "dev-master",
		"vipsoft/unzip": "dev-master",
		"oyatel/amazon-s3-php-class": "dev-master",
		"pda/pheanstalk": "dev-master",
		"phpexcel/phpexcel": "v1.7.7"
	},
	"require-dev": {
		"zizaco/factory-muff": "*",
		"mockery/mockery": "*",
		"alexandresalome/php-selenium": "1.0.1",
		"behat/behat": "2.4.*@dev"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/tests/ControllerTestCase.php",
			"app/tests/AcceptanceTestCase.php",
			"app/tests/data/",
			"app/libraries",
			"app/jobs",
			"app/views"
		]
	},
	"scripts": {
		"pre-update-cmd": "php -r \"@unlink('bootstrap/compiled.php');\"",
	    "post-update-cmd": "php artisan optimize"
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
